{
	"name": "vite-react-typescript-starter",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
		"dev:client": "vite --port 5174",
		"dev:server": "nodemon --watch server --ext ts --exec \"tsx server/index.ts\"",
		"server": "tsx server/index.ts",
		"prebuild": "node scripts/generate-redirects.js",
		"build": "vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit -p tsconfig.app.json",
		"ftd:sync": "tsx server/scripts/runFtdCron.ts",
		"balance:update": "tsx server/scripts/runBalanceCron.ts",
		"shave:migrate": "tsx server/scripts/migrateShaveHistory.ts",
		"generate-icons": "node scripts/generate-icons.js"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.57.4",
		"@types/cookie-parser": "^1.4.9",
		"@types/multer": "^2.0.0",
		"@types/xml2js": "^0.4.14",
		"bcryptjs": "^3.0.2",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"framer-motion": "^11.0.0",
		"lucide-react": "^0.344.0",
		"multer": "^2.0.2",
		"mysql2": "^3.15.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^7.9.4",
		"reactflow": "^11.11.4",
		"resend": "^6.2.0",
		"sonner": "^1.4.0",
		"undici": "^7.16.0",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.1",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/node": "^24.8.1",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.18",
		"concurrently": "^9.2.1",
		"eslint": "^9.9.1",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.11",
		"globals": "^15.9.0",
		"nodemon": "^3.1.10",
		"postcss": "^8.4.35",
		"tailwindcss": "^3.4.1",
		"tsx": "^4.20.6",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.3.0",
		"vite": "^5.4.2",
		"vite-plugin-pwa": "^1.2.0"
	}
}
